{
	"name": "suitecloud-vscode-extension",
	"displayName": "SuiteCloud Extension for Visual Studio Code",
	"description": "Develop and deploy SuiteCloud Projects with SuiteCloud Development Framework (SDF).",
	"keywords": [
		"NetSuite",
		"SDF",
		"SuiteApp",
		"SuiteCloud",
		"SuiteScripts"
	],
	"publisher": "Oracle",
	"license": "UPL-1.0",
	"private": true,
	"version": "3.1.2",
	"icon": "resources/extension_icon_dark.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oracle/netsuite-suitecloud-sdk.git",
		"directory": "packages/vscode-extension"
	},
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/suitecloud.js",
	"contributes": {
		"commands": [
			{
				"command": "suitecloud.adddependencies",
				"title": "Add Dependency References to the Manifest",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.comparefile",
				"title": "Compare with Account File",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.createfile",
				"title": "Create SuiteScript File",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.createproject",
				"title": "Create Project",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.deploy",
				"title": "Deploy Project",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.importfiles",
				"title": "Import Files",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.importobjects",
				"title": "Import Objects",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.listfiles",
				"title": "List Files",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.listobjects",
				"title": "List Objects",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.manageauth",
				"title": "Manage Accounts",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.setupaccount",
				"title": "Set Up Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.updatefile",
				"title": "Update File from Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.updateobject",
				"title": "Update Object from Account",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.uploadfile",
				"title": "Upload File",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.validate",
				"title": "Validate Project",
				"category": "SuiteCloud"
			},
			{
				"command": "suitecloud.opensettings",
				"title": "Open DevAssist settings",
				"category": "SuiteCloud",
				"enablement": "false"
			},
            {
              "command": "suitecloud.opendevassistfeedbackform",
              "title": "Open DevAssist Feedback Form",
              "category": "SuiteCloud",
              "enablement": "false"
            },
            {
              "command": "suitecloud.createdevassistapikey",
              "title": "Create Developer Assistant Service API Key",
              "category": "SuiteCloud",
              "enablement": "false"
            }
		],
		"keybindings": [
			{
				"command": "suitecloud.adddependencies",
				"key": "ctrl+alt+shift+a",
				"mac": "ctrl+alt+cmd+a"
			},
			{
				"command": "suitecloud.deploy",
				"key": "ctrl+alt+d",
				"win": "ctrl+alt+d",
				"mac": "ctrl+alt+cmd+d",
				"linux": "alt+d"
			},
			{
				"command": "suitecloud.importfiles",
				"key": "ctrl+alt+f",
				"mac": "ctrl+alt+f",
				"linux": "ctrl+alt+f"
			},
			{
				"command": "suitecloud.importobjects",
				"key": "ctrl+alt+o",
				"mac": "ctrl+alt+cmd+o",
				"linux": "ctrl+alt+o"
			},
			{
				"command": "suitecloud.updateobject",
				"key": "ctrl+alt+u",
				"mac": "cmd+alt+u",
				"when": "resource =~ /\\/Objects\\/.+\\.xml$/"
			},
			{
				"command": "suitecloud.uploadfile",
				"key": "alt+shift+u",
				"when": "resource =~ /\\/FileCabinet\\/(SuiteScripts|Templates\\/(E-mail%20Templates|Marketing%20Templates)|Web%20Site%20Hosting%20Files|SuiteApps)\\/.+$/"
			},
			{
				"command": "suitecloud.validate",
				"key": "ctrl+alt+v",
				"win": "ctrl+alt+v",
				"mac": "ctrl+alt+cmd+v",
				"linux": "alt+v"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder && resource =~ /\\/FileCabinet\\/(SuiteApps\\/[a-z0-9]+\\.[a-z0-9]+\\.[a-z0-9]+|SuiteScripts|Web%20Site%20Hosting%20Files\\/.+)(\\/.*)*$/",
					"command": "suitecloud.createfile",
					"group": "suitecloud@1"
				},
				{
					"when": "explorerResourceIsFolder && resource =~ /\\/FileCabinet\\/(SuiteScripts|Templates\\/(E-mail%20Templates|Marketing%20Templates)|Web%20Site%20Hosting%20Files)(\\/.*)*$/",
					"command": "suitecloud.importfiles",
					"group": "suitecloud@1"
				},
				{
					"when": "explorerResourceIsFolder && resource =~ /\\/Objects(\\/.+)*$/",
					"command": "suitecloud.importobjects",
					"group": "suitecloud@1"
				},
				{
					"when": "!explorerResourceIsFolder && resource =~ /\\/Objects\\/.+\\.xml$/",
					"command": "suitecloud.updateobject",
					"group": "suitecloud@1"
				},
				{
					"when": "!explorerResourceIsFolder && resource =~ /\\/FileCabinet\\/(SuiteScripts|Templates\\/(E-mail%20Templates|Marketing%20Templates)|Web%20Site%20Hosting%20Files)\\/.+$/",
					"command": "suitecloud.updatefile",
					"group": "suitecloud@1"
				},
				{
					"when": "!explorerResourceIsFolder && resource =~ /\\/FileCabinet\\/(SuiteApps|SuiteScripts|Templates\\/(E-mail%20Templates|Marketing%20Templates)|Web%20Site%20Hosting%20Files)\\/.+$/",
					"command": "suitecloud.comparefile",
					"group": "suitecloud@1"
				}
			]
		},
		"configuration": {
			"title": "SuiteCloud",
			"properties": {
				"suitecloud.developerAssistant.authID": {
					"order": 0,
					"type": "string",
					"default": "authid-to-be-used-by-dev-assist",
					"markdownDescription": "Choose which NetSuite account credentials you want SuiteCloud Developer Assistant service to use. Not sure which auth IDs you have? Open the Command Palette in VS Code, then type and run `SuiteCloud: Manage Accounts` to list them.\n\nWant more information on setting up a NetSuite account? See [NetSuite Help Center](https://nlcorp.app.netsuite.com/app/help/helpcenter.nl?fid=section_160147609118.html)."
				},
				"suitecloud.developerAssistant.localPort": {
					"order": 1,
					"type": "number",
					"default": 8181,
					"description": "Enter the local port for SuiteCloud Developer Assistant service."
				},
				"suitecloud.developerAssistant.enable": {
					"order": 2,
					"type": "boolean",
					"default": false,
					"markdownDescription": "Check this box to enable SuiteCloud Developer Assistant service.\n\n**Disclaimer:** The content is generated by AI and may contain errors or omissions. Review output before use. It is intended for informational purposes only and should not be considered professional advice.\n\nTo make it available in Cline, go to Cline settings and update the following:\n\n* `API Provider`: Set to `OpenAI Compatible`.\n* `Base URL`: Set to `http://127.0.0.1:[local_port]/api/internal/devassist`.\n* `OpenAI Compatible API Key`: Paste the generated API key.\n\nWant more information on setting up SuiteCloud Developer Assistant? See [NetSuite Help Center](https://nlcorp.app.netsuite.com/app/help/helpcenter.nl?fid=article_0916095619.html)."
				},
				"suitecloud.developerAssistant.disableWelcomeNotification": {
					"order": 3,
					"type": "boolean",
					"default": false,
					"description": "Check this box if you don't want to receive a welcome notification for SuiteCloud Developer Assistant service."
				}
			}
		},
		"icons": {
			"netsuite-mobius-colorless-icon": {
				"description": "NetSuite Logo Text Icon",
				"default": {
					"fontPath": "resources/icon/SuiteCloudSDKTextIcons.woff",
					"fontCharacter": "\\E900"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"@oracle/suitecloud-cli": "^3.1.2"
	},
	"devDependencies": {
		"@types/mocha": "10.0.10",
		"@types/node": "20.17.14",
		"@types/vscode": "1.96.0",
		"@typescript-eslint/eslint-plugin": "8.20.0",
		"@typescript-eslint/parser": "8.20.0",
		"@vscode/test-electron": "2.4.1",
		"eslint": "9.18.0",
		"eslint-config-prettier": "10.0.1",
		"mocha": "11.1.0",
		"typescript": "5.7.3"
	}
}
